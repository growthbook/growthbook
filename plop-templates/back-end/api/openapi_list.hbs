summary: Get all {{object}}
tags:
  - {{kebabCase object}}s
parameters:
- $ref: "../parameters.yaml#/limit"
- $ref: "../parameters.yaml#/offset"
operationId: list{{pascalCase object}}s
x-codeSamples:
  - lang: 'cURL'
    source: |
      curl https://api.growthbook.io/api/v1/{{kebabCase object}}s \
        -u secret_abc123DEF456:
responses:
  "200":
    content:
      application/json:
        schema:
          allOf:
            - type: object
              required:
                - {{camelCase object}}s
              properties:
                {{camelCase object}}s:
                  type: array
                  items:
                    $ref: "../schemas/{{pascalCase object}}.yaml"
            - $ref: "../schemas/PaginationFields.yaml"
