get:
  summary: Get all {{object}}s
  parameters:
  - $ref: "../../parameters/limit.yml"
  - $ref: "../../parameters/offset.yml"
  operationId: list{{pascalCase object}}s
  responses:
    "200":
      content:
        application/json:
          schema:
            allOf:
              - type: object
                required:
                  - {{camelCase object}}s
                properties:
                  {{camelCase object}}s:
                    type: array
                    items:
                      $ref: "../../schemas/{{pascalCase object}}.yml"
              - $ref: "../../schemas/PaginationFields.yml"