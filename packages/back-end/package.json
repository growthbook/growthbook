{
  "name": "back-end",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build:emails": "mkdir -p dist/templates && cp -r src/templates/* dist/templates",
    "build:clean": "rimraf -rf dist",
    "build:typescript": "tsc",
    "clean": "rimraf node_modules",
    "dev": "yarn build:clean && concurrently \"yarn watch-compile\" \"yarn watch-dev | yarn pino-pretty\"",
    "build": "yarn build:clean && yarn build:typescript && yarn build:emails",
    "start": "node dist/server.js",
    "start:with-tracing": "node --require @opentelemetry/auto-instrumentations-node/register dist/server.js",
    "repl": "node --require ts-node/register/transpile-only --experimental-repl-await repl",
    "test": "jest --forceExit --verbose --detectOpenHandles",
    "type-check": "tsc --pretty --noEmit",
    "unused-export-check": "npx ts-unused-exports tsconfig.json --findCompletelyUnusedFiles --allowUnusedTypes --showLineNumber",
    "generate-dummy-data": "ts-node --swc ./test/data-generator/new-generator.ts",
    "test-integration-queries": "./test/integrations/integration-query-test.sh",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "delay 1 && nodemon --watch dist --watch '../shared/dist' --watch '../enterprise/dist' -e js ./dist/server.js",
    "migrate-encryption-key": "node dist/scripts/migrate-encryption-key.js",
    "generate-api-models": "node src/scripts/generate-doc-models-for-openapi-schemas.mjs",
    "generate-api-types": "yarn generate-api-models && swagger-cli bundle -t yaml src/api/openapi/openapi.tmp.yaml -o generated/spec.yaml && node src/scripts/generate-openapi.mjs"
  },
  "dependencies": {
    "@databricks/sql": "^1.1.0",
    "@google-cloud/bigquery": "5",
