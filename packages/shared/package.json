{
  "name": "shared",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "src/index.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:typescript": "tsc",
    "dev": "swc src -w --out-dir dist",
    "build": "pnpm build:clean && pnpm build:typescript",
    "test": "jest --forceExit --verbose --detectOpenHandles",
    "clean": "rimraf node_modules",
    "type-check": "tsc --pretty --noEmit",
    "generate-sdk-report": "node -r @swc-node/register ./src/sdk-versioning/sdk-report.ts ./src/sdk-versioning/CAPABILITIES.md",
    "gen-event-webhook-for-docs": "pnpm exec tsx ./scripts/gen-event-webhook-doc.ts",
    "gen-sdk-resources-for-docs": "pnpm build && pnpm exec tsx ./scripts/gen-sdk-resources.ts",
    "gen-commercial-features-for-docs": "pnpm build && pnpm exec tsx ./scripts/gen-commercial-features-for-docs.ts"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.21",
    "@google-cloud/bigquery": "^8.1.1",
    "@growthbook/growthbook": "^1.6.5",
    "@stdlib/stats": "^0.1.1",
    "ajv": "^8.12.0",
    "date-fns": "^2.15.0",
    "date-fns-tz": "^1.3.7",
    "dirty-json": "^0.9.2",
    "json-stringify-pretty-compact": "^3.0.0",
    "lodash": "^4.17.23",
    "openid-client": "5.4.3",
    "shared": "workspace:*",
    "sql-formatter": "^15.7.2",
    "stripe": "17.7.0",
    "uniqid": "^5.4.0",
    "zod": "^4.1.12"
  },
  "optionalDependencies": {
    "@polyglot-sql/sdk": "^0.1.7"
  },
  "devDependencies": {
    "@ephys/zod-to-ts": "^2.3.2",
    "@swc-node/register": "^1.9.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.4",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^27.0.1",
    "@types/lodash": "4.14.169",
    "@types/node": "^22.8.6",
    "@types/uniqid": "5.3.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "jest": "^27.1.1",
    "prettier": "3.6.2",
    "rimraf": "^3.0.2",
    "tsx": "^4.7.0",
    "typescript": "5.7.3"
  },
  "license": "Mixed"
}
