{
  "name": "shared",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./ai": {
      "types": "./src/ai.ts",
      "default": "./dist/ai.js"
    },
    "./constants": {
      "types": "./src/constants.ts",
      "default": "./dist/constants.js"
    },
    "./dates": {
      "types": "./src/dates.ts",
      "default": "./dist/dates.js"
    },
    "./experiments": {
      "types": "./src/experiments.ts",
      "default": "./dist/experiments.js"
    },
    "./enterprise": {
      "types": "./src/enterprise/index.ts",
      "default": "./dist/enterprise/index.js"
    },
    "./health": {
      "types": "./src/health/index.ts",
      "default": "./dist/health/index.js"
    },
    "./permissions": {
      "types": "./src/permissions/index.ts",
      "default": "./dist/permissions/index.js"
    },
    "./power": {
      "types": "./src/power/index.ts",
      "default": "./dist/power/index.js"
    },
    "./sdk-versioning": {
      "types": "./src/sdk-versioning/index.ts",
      "default": "./dist/sdk-versioning/index.js"
    },
    "./settings": {
      "types": "./src/settings/index.ts",
      "default": "./dist/settings/index.js"
    },
    "./sql": {
      "types": "./src/sql.ts",
      "default": "./dist/sql.js"
    },
    "./util": {
      "types": "./src/util/index.ts",
      "default": "./dist/util/index.js"
    },
    "./demo-datasource": {
      "types": "./src/demo-datasource/index.ts",
      "default": "./dist/demo-datasource/index.js"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:typescript": "tsc",
    "dev": "swc src -w --out-dir dist",
    "build": "yarn build:clean && yarn build:typescript",
    "test": "jest --forceExit --verbose --detectOpenHandles",
    "clean": "rimraf node_modules",
    "type-check": "tsc --pretty --noEmit",
    "unused-export-check": "knip --directory ../.. --workspace packages/shared",
    "generate-sdk-report": "node -r @swc-node/register ./src/sdk-versioning/sdk-report.ts ./src/sdk-versioning/CAPABILITIES.md"
  },
  "dependencies": {
    "@growthbook/growthbook": "^1.6.2",
    "ajv": "^8.12.0",
    "date-fns": "^2.15.0",
    "dirty-json": "^0.9.2",
    "json-stringify-pretty-compact": "^3.0.0",
    "node-gyp": "10.3.1",
    "sql-formatter": "^11.0.2",
    "uniqid": "^5.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@swc-node/register": "^1.9.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.4",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^27.0.1",
    "@types/node": "^22.8.6",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "jest": "^27.1.1",
    "rimraf": "^3.0.2",
    "typescript": "5.7.3"
  },
  "license": "Mixed"
}
