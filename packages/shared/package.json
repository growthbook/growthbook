{
  "name": "shared",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./ai": {
      "types": "./src/ai.ts",
      "default": "./dist/ai.js"
    },
    "./constants": {
      "types": "./src/constants.ts",
      "default": "./dist/constants.js"
    },
    "./dates": {
      "types": "./src/dates.ts",
      "default": "./dist/dates.js"
    },
    "./demo-datasource": {
      "types": "./src/demo-datasource/index.ts",
      "default": "./dist/demo-datasource/index.js"
    },
    "./enterprise": {
      "types": "./src/enterprise/index.ts",
      "default": "./dist/enterprise/index.js"
    },
    "./experiments": {
      "types": "./src/experiments.ts",
      "default": "./dist/experiments.js"
    },
    "./health": {
      "types": "./src/health/index.ts",
      "default": "./dist/health/index.js"
    },
    "./permissions": {
      "types": "./src/permissions/index.ts",
      "default": "./dist/permissions/index.js"
    },
    "./power": {
      "types": "./src/power/index.ts",
      "default": "./dist/power/index.js"
    },
    "./sdk-versioning": {
      "types": "./src/sdk-versioning/index.ts",
      "default": "./dist/sdk-versioning/index.js"
    },
    "./settings": {
      "types": "./src/settings/index.ts",
      "default": "./dist/settings/index.js"
    },
    "./sql": {
      "types": "./src/sql.ts",
      "default": "./dist/sql.js"
    },
    "./util": {
      "types": "./src/util/index.ts",
      "default": "./dist/util/index.js"
    },
    "./validators": {
      "types": "./src/validators/index.ts",
      "default": "./dist/validators/index.js"
    },
    "./types": {
      "types": "./types.d.ts",
      "default": "./types.js"
    },
    "./types/*": {
      "types": "./types/*.d.ts",
      "default": "./types/*.js"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:typescript": "tsc",
    "build:gen-sdk-data": "node scripts/generate-sdk-versions-data.cjs",
    "dev": "pnpm build:gen-sdk-data && pnpm build:clean && swc src --out-dir dist && swc src -w --out-dir dist",
    "build": "pnpm build:gen-sdk-data && pnpm build:clean && pnpm build:typescript",
    "test": "jest --forceExit --verbose --detectOpenHandles",
    "clean": "rimraf node_modules",
    "type-check": "tsc --pretty --noEmit",
    "generate-sdk-report": "node -r @swc-node/register ./src/sdk-versioning/sdk-report.ts ./src/sdk-versioning/CAPABILITIES.md",
    "gen-event-webhook-for-docs": "pnpm exec tsx ./scripts/gen-event-webhook-doc.ts",
    "gen-sdk-resources-for-docs": "pnpm build && pnpm exec tsx ./scripts/gen-sdk-resources.ts"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.21",
    "@google-cloud/bigquery": "^8.1.1",
    "@growthbook/growthbook": "^1.6.4",
    "@stdlib/stats": "^0.1.1",
    "ajv": "^8.12.0",
    "date-fns": "^2.15.0",
    "date-fns-tz": "^1.3.7",
    "dirty-json": "^0.9.2",
    "json-stringify-pretty-compact": "^3.0.0",
    "lodash": "^4.17.23",
    "openid-client": "5.4.3",
    "shared": "workspace:*",
    "sql-formatter": "^11.0.2",
    "stripe": "17.7.0",
    "uniqid": "^5.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@ephys/zod-to-ts": "^2.3.2",
    "@swc-node/register": "^1.9.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.4",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^27.0.1",
    "@types/lodash": "4.14.169",
    "@types/node": "^22.8.6",
    "@types/uniqid": "5.3.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "jest": "^27.1.1",
    "prettier": "3.6.2",
    "rimraf": "^3.0.2",
    "tsx": "^4.7.0",
    "typescript": "5.7.3"
  },
  "license": "Mixed"
}
